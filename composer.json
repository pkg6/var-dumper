{
  "name": "pkg6/var-dumper",
  "type": "library",
  "license": "MIT",
  "description": "VarDumper",
  "keywords": [
    "var-dumper"
  ],
  "authors": [
    {
      "name": "zhiqiangwang",
      "email": "zhiqiangwang@aliyun.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "ext-json": "*",
